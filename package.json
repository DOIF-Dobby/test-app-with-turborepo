{
  "name": "test-app-with-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:service": "turbo dev --filter=service",
    "build:service": "turbo build --filter=service",
    "start:service": "turbo start --filter=service",
    "dev:docs": "turbo dev --filter=docs",
    "build:docs": "turbo build --filter=docs",
    "start:docs": "turbo start --filter=docs",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "turbo": "^2.5.6",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
